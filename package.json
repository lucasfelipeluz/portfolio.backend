{
  "name": "portfolio.backend",
  "version": "0.0.1",
  "main": "src/app.js",
  "scripts": {
    "start": "node build/index.js",
    "build": "tsc && tsc-alias",
    "dev": "nodemon 'src/' --exec 'ts-node -r tsconfig-paths/register src/index.ts' -e ts",
    "create-migration": "npx sequelize-cli migration:create --migrations-path 'src/infrastructure/migrations' --name",
    "push-migration": "npm run build && npx sequelize-cli db:migrate",
    "undo-migration": "npm run build && npx sequelize-cli db:migrate:undo"
  },
  "author": "Lucas Felipe Luz",
  "license": "ISC",
  "description": "My backend's portfolio",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.5",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.1",
    "sequelize-cli": "^6.6.2",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "mysql2": "^3.10.1",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.3",
    "tsyringe": "^4.8.0"
  }
}
